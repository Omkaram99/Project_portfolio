ExpiresByType text/css "access 0 minutes"
ExpiresByType text/html "access 0 minutes"
ExpiresByType text/x-javascript "access 0 minutes"
ExpiresByType text/javascript "access  0 minutes"
ExpiresByType text/xml "access 0 minutes"

ExpiresByType application/pdf "access 0 minutes"
